<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.icollection.location">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <uses-permission android:name="android.permission.VIBRATE" />

    <!--上传照片-->
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".WebViewActivity" />

        <activity android:name=".Location.LocationActivity" />

        <activity android:name=".Location.LocationEBActivity" />

        <activity android:name=".Delivery.DeliveryAffairActivity" />

        <activity android:name=".Delivery.CashActivity" />

        <activity android:name=".Delivery.DeliveryAffair.DeliveryActivity" />

        <activity android:name=".Delivery.DeliveryAffair.DeliveryAddActivity" />

        <activity android:name=".Delivery.Store.StoreDeliveryActivity" />

        <activity android:name=".Delivery.Store.StoreDeliveryAddActivity" />

        <activity android:name=".ShopStock.ShopStockActivity" />


    </application>

</manifest>